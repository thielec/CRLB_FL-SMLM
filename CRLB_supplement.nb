  RowBox[{
   FormBox["f",
    TraditionalForm], " ", "=", " ", 
   TagBox[
    RowBox[{
     FormBox[
      RowBox[{"(", "\[NoBreak]", GridBox[{
         {
          SubscriptBox["f", "\[Tau]\[Tau]"], 
          SubscriptBox["f", "\[Tau]b"], 
          SubscriptBox["f", "\[Tau]N"]},
         {
          SubscriptBox["f", "\[Tau]b"], 
          SubscriptBox["f", "bb"], 
          SubscriptBox["f", "bN"]},
         {
