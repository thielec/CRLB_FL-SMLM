Cell["\<\
Mathematica needs some help with the simplification : We need to reduce all \
Log expressions and collect the \[Tau] to enable cancelling them. 
For efficient numerical evaluation, Powers are expanded again to avoid errors \
by to large arguments in the Log. The terms with PolyLog are grouped to \
minimise calls to PolyLog.\
\>", "Text",ExpressionUUID->"c4e8396e-79a5-4266-8885-fc537a3b505c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"SimplifyCRLB", "[", "exprs_", "]"}], ":=", 
  RowBox[{"Assuming", "[", 
   RowBox[{"pAssumptions", ",", "\[IndentingNewLine]", 
    RowBox[{"Simplify", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Simplify", "[", 
       RowBox[{"exprs", "//.", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"Log", "[", "a_", "]"}], "+", 
            RowBox[{"Log", "[", "b_", "]"}]}], "\[RuleDelayed]", " ", 
           RowBox[{"Log", "[", 
            RowBox[{"a", "*", "b"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"x_", "*", 
            RowBox[{"Log", "[", "a_", "]"}]}], "\[RuleDelayed]", " ", 
           RowBox[{"Log", "[", 
            RowBox[{"a", "^", "x"}], "]"}]}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"a_", "*", "\[Tau]"}], "+", 
            RowBox[{"b_", "*", "\[Tau]"}]}], "\[RuleDelayed]", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"a", "+", "b"}], ")"}], "*", "\[Tau]"}]}]}], "}"}]}], 
       "]"}], " ", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Cancel", " ", "\[Tau]", " ", "in", " ", "the", " ", "log", " ", 
        "terms"}], " ", "*)"}], "\[IndentingNewLine]", "//.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"Log", "[", 
          RowBox[{"a_", "*", 
           RowBox[{"b_", "^", "x_"}]}], "]"}], "\[RuleDelayed]", " ", 
         RowBox[{
          RowBox[{"Log", "[", "a", "]"}], "+", 
          RowBox[{"x", "*", 
           RowBox[{"Log", "[", "b", "]"}]}]}]}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"a_", "*", 
           RowBox[{"PolyLog", "[", 
            RowBox[{"x_", ",", "c_"}], "]"}]}], "+", 
          RowBox[{"b_", "*", 
           RowBox[{"PolyLog", "[", 
            RowBox[{"x_", ",", "c_"}], "]"}]}]}], "\[RuleDelayed]", " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"a", "+", "b"}], ")"}], "*", 
          RowBox[{"PolyLog", "[", 
           RowBox[{"x", ",", "c"}], "]"}]}]}]}], "}"}]}], "]"}]}], 
   RowBox[{"(*", " ", 
    RowBox[{"Optimise", " ", "for", " ", "numerical", " ", "evaluation"}], 
    " ", "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"18eacca8-6745-467d-9848-51c80d1a7623"],

